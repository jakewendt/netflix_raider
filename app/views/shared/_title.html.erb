	<tr id="<%=dom_id(title)-%>" class="<%=dom_class(title)-%>">

<% if @user %>
		<td id='user_rating_<%=title.id-%>' style='text-align:center'>
			<%= user_star_bar(title.user_rating,title.id) -%></td>
		<td id='predicted_rating_<%=title.id-%>'>
			<%= star_bar(title.predicted_rating) -%>
		</td>
<% end %>
		<td><%= star_bar(title.average_rating||0) -%></td>
		<td style='text-align:center'><%= title.release_year||0 -%></td>
<% if @user and @user.is_jake? %>
		<td style='text-align:center'>
			<%= link_to image_tag('application_edit.png', :title => 'Edit Title'), edit_title_path(title) -%>
		</td>
		<td style='text-align:center'>
			<%= link_to_remote image_tag('accept.png', :title => 'Validate Title'), :url => validate_title_path(title) -%>
		</td>
<% end %>
		<td><%= link_to title.title, title.web_page -%></td>
		<td><%= (!title.status.blank?) ? title.status : "&nbsp;" -%></td>
	</tr>
